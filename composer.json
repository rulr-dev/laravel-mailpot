{
    "name": "rulr/mailpot",
    "description": "A local mail inbox for Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rulr\\Mailpot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rulr\\Mailpot\\MailpotServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/mailer": "^6.0 || ^7.0"
    }
}
