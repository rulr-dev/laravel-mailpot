{
    "name": "rulr/laravel-mailpot",
    "description": "A local mail inbox for Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rulr\\Mailpot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rulr\\Mailpot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rulr\\Mailpot\\MailpotServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/mailer": "^6.0 || ^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^12.2"
    }
}
